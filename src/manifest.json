{
  "name": "GohanVt",
  "description": "A Vue.js web extension",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "browser_action": {
    "default_title": "GohanVt",
    "default_popup": "popup/popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "tab/tab.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "bookmarks",
    "geolocation",
    "notifications",
    "topSites",
    "contextMenus",
    "unlimitedStorage",
    "scripting",
    "https://ajax.googleapis.com/",
    "https://*/*",
    "http://*/*",
    "chrome://favicon/"
  ],
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
